<page>
  <view class="box5" style="position:relative;" bindtap='PersonalInfo'>
    <!-- 背景图的view -->
    <image class="img2" src="http://wx3.sinaimg.cn/small/9bba9548gy1fm6vp2k8llj20sg0hiago.jpg"  />
    <image class="img1" src="http://wx1.sinaimg.cn/small/9bba9548gy1fm6vp7q4n4j20p10p1mxl.jpg"  />
    <view class="FONT4 personName">姓名：{{StudentName}}</view>
    <view class="FONT4 number">学号：{{StudentNumber}}</view>
  </view>
  
  <view class="box3" wx:for='{{ClassesInformation}}' bindtap='ChooseClass3' data-i="{{index}}">
    <text class="FONT11" > {{Classes[index]}}</text>
    <text class="FONT51"> 老师：{{ClassesTeacher[index]}}</text>
  </view>
</page>